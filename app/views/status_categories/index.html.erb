<%- model_class = StatusCategory -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped DataTables">
  <thead>
    <tr>
      <th>ID</th>
      <th>Status Category Name</th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @status_categories.each do |status_category| %>
      <tr>
        <td><%= link_to status_category.id, status_category_path(status_category) %></td>
        <td><%= status_category.status_category_name %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_status_category_path(status_category), :class => 'fa fa-edit fa-2 btn btn-default btn-sm' %>

        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_status_category_path,
            :class => 'btn btn-primary btn-index' %>
