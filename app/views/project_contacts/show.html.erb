<%- model_class = ProjectContact -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>
<div class ="backgroundContainerProjectContact">
  <fieldset>
    <legend>General Information</legend>

<dl class="dl-horizontal">
  <dt><strong>Project Name:</strong></dt>
  <dd><%= @project_contact.project.project_name %></dd>
  <dt><strong>Contact Name:</strong></dt>
  <dd><%= @project_contact.contact.full_contact_name %></dd>
</dl>
  </fieldset>
</div>
<div class = "showPageButton">
<%= link_to t('.back', :default => t("helpers.links.back")),
              project_contacts_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_project_contact_path(@project_contact), :class => 'btn btn-default' %>

</div>