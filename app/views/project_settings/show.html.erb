<%- model_class = ProjectSetting -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong>Project Setting Name:</strong></dt>
  <dd><%= @project_setting.project_setting_name %></dd>
</dl>

<div class = "showPageButton">
<%= link_to t('.back', :default => t("helpers.links.back")),
              project_settings_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_project_setting_path(@project_setting), :class => 'fa fa-edit fa-2 btn btn-default' %>

</div>