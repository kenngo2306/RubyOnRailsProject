</body><div class="container">
  <div class="row vertical-offset-100">
    <div class="col-md-4 col-md-offset-4">
      <div class="panel panel-default">
        <div class="panel-heading">
          <div class="row-fluid user-row">
            <img src="http://dev.itvgoggles.com/wp-content/uploads/2014/02/caduceus-alone.png"
                 class="img-responsive" alt="Conxole Admin" width="150px"/>
          </div>
        </div>
        <div class="panel-body">
          <%= form_for(resource, as: resource_name, :class => "navbar-form navbar-left form-inline form-signin", :role => "form", url: session_path(resource_name)) do |f|%>
              <%= f.label :email, :class => 'login-label' %><br />
              <%= f.email_field :email, autofocus: true, :class => 'form-control' %>
              <%= f.label :password, :class => 'login-label' %><br />
              <%= f.password_field :password, autocomplete: "off", :class => 'form-control'%>
              <%- if devise_mapping.recoverable? %>
                  <%= link_to "Forgot password?", new_password_path(resource_name), class: 'login-label' %>
                  </br></br>
                  <button class="btn btn-lg btn-primary btn-block" type="submit">Sign in</button>
                  <div class="checkbox">
                    <label class="remember">
                      <input type="checkbox" value="remember-me" class="character-checkbox">
                      Remember me
                    </label>
                  </div>
              <% end -%>
              <%= render 'shared/loginlinks' %><span class="clearfix"></span><% end %>
        </div>
      </div>
    </div>
  </div>
</div>
</body>

