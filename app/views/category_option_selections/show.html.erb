<%- model_class = CategoryOptionSelection -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:category_option_selection_name) %>:</strong></dt>
  <dd><%= @category_option_selection.category_option_selection_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:category_option_id) %>:</strong></dt>
  <dd><%= @category_option_selection.category_option_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:project_type_information_id) %>:</strong></dt>
  <dd><%= @category_option_selection.project_type_information_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              category_option_selections_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_category_option_selection_path(@category_option_selection), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              category_option_selection_path(@category_option_selection),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
