<%- model_class = CategoryOptionSelection -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped DataTables">
  <thead>
    <tr>
      <th>ID</th>
      <th>Category Option Selection Name</th>
      <th>Category Option</th>
      <th>Project Type Information</th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @category_option_selections.each do |category_option_selection| %>
      <tr>
        <td><%= link_to category_option_selection.id, category_option_selection_path(category_option_selection) %></td>

        <td><%= category_option_selection.category_option_id %></td>
        <td><%= category_option_selection.project_type_information_id %></td>
        <td>
          <%= link_to t('.show', :default => t("helpers.links.show")),
                      category_option_selection, :class => ' btn-default btn-sm'  %>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_category_option_selection_path(category_option_selection), :class => 'btn-default btn-sm' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      category_option_selection_path(category_option_selection),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => ' btn-sm btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_category_option_selection_path,
            :class => 'btn btn-primary btn-index' %>
