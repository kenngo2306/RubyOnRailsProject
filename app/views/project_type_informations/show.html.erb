<%- model_class = ProjectTypeInformation -%>
<div class="page-header">
  <h1>Design Information</h1>
</div>
<%= link_to 'Manage Project Site', project_site_information_path(@project_type_information.project_site_information), class: 'btn-primary btn-sm' %>
<%= link_to 'Manage Project', project_path(@project_type_information.project_site_information.project), class: 'btn-primary btn-sm' %>
<dl class="dl-horizontal">
  <dt><strong>Proposal Number:</strong></dt>
  <dd><%= @project_type_information.proposal_number %></dd>
  <dt><strong>Revision Number:</strong></dt>
  <dd><%= @project_type_information.revision_number %></dd>

  <dt><strong>Project Type ID:</strong></dt>
  <dd><%= @project_type_information.project_type.project_type_name %></dd>
</dl>

<div class ="showPageButton">
<%= link_to t('.back', :default => t("helpers.links.back")),
              project_type_informations_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_project_type_information_path(@project_type_information), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              project_type_information_path(@project_type_information),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>



