<%- model_class = ProjectType -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped DataTables">
  <thead>
    <tr>
      <th>ID</th>
      <th>Project Type Name</th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @project_types.each do |project_type| %>
      <tr>
        <td><%= link_to project_type.id, project_type_path(project_type) %></td>
        <td><%= project_type.project_type_name %></td>

        <td>
          <%= link_to t('.show', :default => t("helpers.links.show")),
                      project_type_path(project_type), :class => 'btn-default btn-sm' %>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_project_type_path(project_type), :class => ' btn-default btn-sm' %>

        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_project_type_path,
            :class => 'btn btn-primary btn-index' %>
