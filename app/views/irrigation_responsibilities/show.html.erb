<%- model_class = IrrigationResponsibility -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong>Irrigation Responsibility Name:</strong></dt>
  <dd><%= @irrigation_responsibility.irrigation_responsibility_name %></dd>
</dl>

<div class = "showPageButton">
<%= link_to t('.back', :default => t("helpers.links.back")),
              irrigation_responsibilities_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_irrigation_responsibility_path(@irrigation_responsibility), :class => 'btn btn-default' %>

</div>