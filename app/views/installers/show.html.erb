<%- model_class = Installer -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:installer_first_name) %>:</strong></dt>
  <dd><%= @installer.installer_first_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:installer_last_name) %>:</strong></dt>
  <dd><%= @installer.installer_last_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:installer_email) %>:</strong></dt>
  <dd><%= @installer.installer_email %></dd>
  <dt><strong><%= model_class.human_attribute_name(:installer_phone) %>:</strong></dt>
  <dd><%= @installer.installer_phone %></dd>
  <dt><strong><%= model_class.human_attribute_name(:user_id) %>:</strong></dt>
  <dd><%= @installer.user_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              installers_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_installer_path(@installer), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              installer_path(@installer),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
