<%- model_class = ContactType -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong>Contact Type Name:</strong></dt>
  <dd><%= @contact_type.contact_type_name %></dd>
</dl>

<div class = "showPageButton">
<%= link_to t('.back', :default => t("helpers.links.back")),
              contact_types_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_contact_type_path(@contact_type), :class => 'fa fa-edit fa-2 btn btn-default' %>

</div>