  <dt><strong><%= model_class.human_attribute_name(:sales_person_id) %>:</strong></dt>
  <dd><%= @project.sales_person_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:project_status_id) %>:</strong></dt>
  <dd><%= @project.project_status_id %></dd>
