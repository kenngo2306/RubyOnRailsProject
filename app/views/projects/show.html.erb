  <dd><%= @project.time_frame_description %></dd>
  <dt><strong><%= model_class.human_attribute_name(:mobilization_quantity) %>:</strong></dt>
  <dd><%= @project.mobilization_quantity %></dd>
  <dt><strong><%= model_class.human_attribute_name(:liquidate_damages_yn) %>:</strong></dt>
